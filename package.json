{
  "name": "pg-generator-example",
  "version": "1.0.0",
  "description": "Example generator for pg-generator.",
  "author": {
    "name": "Özüm Eldoğan",
    "email": "ozum@ozum.net",
    "url": "https://www.ozum.net/"
  },
  "keywords": [
    "pg-generator",
    "orm",
    "sequelize",
    "md",
    "erd"
  ],
  "engines": {
    "node": ">= 12.0.0"
  },
  "main": "generators/index.js",
  "types": "generators/index.d.ts",
  "files": [
    "generators",
    "!generators/**/@(*.spec|*.test)*",
    "!generators/**/__test__",
    "module-files"
  ],
  "homepage": "https://github.com/ozum/pg-generator-example",
  "bugs": "https://github.com/ozum/pg-generator-example/issues",
  "repository": "ozum/pg-generator-example",
  "license": "MIT",
  "scripts": {
    "execute": "ts-node --transpile-only --compiler-options '{ \"module\": \"commonjs\" }'",
    "watch": "ts-node-dev --no-notify --respawn --transpile-only --compiler-options '{ \"module\": \"commonjs\" }'",
    "lint": "eslint --ignore-path .gitignore --cache --max-warnings 0 --ext js,jsx,ts,tsx,vue --fix .",
    "format": "prettier . --write",
    "release": "npm run readme && git pull && git add -A && git commit && git push --follow-tags",
    "pretest": "not-sync coverage",
    "test": "jest --coverage",
    "readme": "node module-files/scripts/tsmod.js readme",
    "prebuild": "rm -rf generators && copyup --all --exclude \"**/*.ts\" \"src/**/*\" generators",
    "build": "tsc",
    "preinstall": "npx not-sync node_modules",
    "postinstall": "is-ci || husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "tsmod": "node module-files/scripts/tsmod.js",
    "yo:update": "yo tsmod:uninstall --no-install --force && yo 'tsmod' '--projectRoot' 'generators' '--noCoverage'"
  },
  "dependencies": {
    "js-base64": "^3.6.0",
    "lodash.merge": "^4.6.2",
    "nunjucks": "^3.2.3",
    "pg-generator": "^5.0.0-alpha.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ozum/pinst": "^2.1.4",
    "@types/jest": "^26.0.20",
    "@types/lodash.merge": "^4.6.6",
    "@types/node": "^14.14.31",
    "@types/nunjucks": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "commitizen": "^4.2.3",
    "concat-md": "^0.3.5",
    "copyfiles": "^2.4.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^5.0.9",
    "is-ci": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "not-sync": "^1.4.1",
    "prettier": "^2.2.1",
    "readmeasy": "^0.2.7",
    "ts-jest": "^26.5.1",
    "ts-node-dev": "^1.1.1",
    "typedoc": "^0.20.27",
    "typedoc-neo-theme": "^1.1.0",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-markdown": "^3.5.0",
    "typedoc-plugin-param-names": "^2.0.0",
    "typescript": "^4.1.5",
    "walkdir": "^0.4.1"
  }
}
